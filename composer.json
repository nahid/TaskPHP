{
    "name": "nahid/task-php",
    "description": "Framework-agnostic PHP concurrency package using child processes",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "laravel/serializable-closure": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Nahid\\TaskPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nahid\\TaskPHP\\Tests\\": "tests/",
            "Nahid\\TaskPHP\\Examples\\": "examples/"
        }
    },
    "authors": [
        {
            "name": "Nahid Bin Azhar",
            "email": "nahid.dns@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}